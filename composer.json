{
    "name": "levoolabs/imageable",
    "description": "Eloquent Image model for uploading and displaying images with intervention/imagecache",
    "keywords": ["image", "intervention", "eloquent", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Troll",
            "email": "adam.troll@levoolabs.com"
        },
        {
            "name": "Peter Cserni",
            "email": "peter.cserni@levoolabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LevooLabs\\Imageable\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.4",
        "illuminate/support": "^5.4",
        "illuminate/http": "^5.4",
        "illuminate/foundation": "^5.4",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LevooLabs\\Imageable\\ImageableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
